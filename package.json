    "test:unit": "jest --selectProjects unit",
    "test:unit:coverage": "jest --selectProjects unit --coverage",
    "test:feature": "cucumber-js",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:load:local": "artillery run __tests__/load/local.basic-load-test.yml",
    "prisma:update": "npm i --save-dev prisma@latest && npm i @prisma/client@latest",
    "prisma:studio": "prisma studio",
